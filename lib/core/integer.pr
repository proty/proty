class Integer < Object
    value : prim_int

    def init(value : prim_int)
        @value = value

    def +(other : Integer)
        return Integer.new(@value + other.value)

    def -(other : Integer)
        return Integer.new(@value - other.value)

    def *(other : Integer)
        return Integer.new(@value * other.value)

    def /(other : Integer)
        return Integer.new(@value / other.value)