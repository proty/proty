test:run("basic math operators", do(test) {
    test.assertEqual(5+5, 10)
    test.assertEqual(15-5, 10)
    test.assertEqual(5*2, 10)
    test.assertEqual(50/5, 10)
    test.assertEqual(5+2*10, 25)
    test.assertEqual(5+2*10+1, 26)
})

test:run("relational operators", do(test) {
    test.assertTrue(5<10)
    test.assertTrue(10>5)
    test.assertTrue(5<=5)
    test.assertTrue(5>=5)
    test.assertTrue(5==5)
    test.assertTrue(5!=10)
})